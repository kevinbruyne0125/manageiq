<% if @sb[:active_tab] == "analytics_details" %>
	<% if @sb[:analytics_rpt] %>
		<fieldset>
			<p class="legend"><%= @sb[:analytics_rpt].title %></p>
			<% unless @sb[:analytics_rpt].table.nil? || @sb[:analytics_rpt].table.data.empty? %>
				<%= @sb[:analytics_rpt].to_html.html_safe %>
			<% else %>
				No records found.
			<% end %>
		</fieldset>
	<% end %>
<% end %>

