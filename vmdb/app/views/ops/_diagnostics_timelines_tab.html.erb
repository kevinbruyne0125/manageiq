<% if @sb[:active_tab] == "diagnostics_timelines" %>
	<%= render :partial => "layouts/flash_msg" %>
	<% if @tl_record %>
		<script type="text/javascript">
			<%# Create from/to date JS vars to limit calendar selection range %>
			var miq_cal_dateFrom = new Date(<%= @tl_options[:sdate] %>);
			var miq_cal_dateTo = new Date(<%= @tl_options[:edate] %>);
		</script>
		<%= render(:partial => 'layouts/tl_options') %>
		<%= render :partial=>'layouts/tl_detail' %>
	<% else %>
		<div class="alert alert-info">
			<span class="pficon pficon-info"></span>
			<strong>This Server's Virtual Machine has not been identified.</strong>
		</div>
	<% end %>
	<div style="padding-bottom:20px;"></div>
<% end %>
