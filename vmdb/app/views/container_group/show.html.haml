- if @display == 'containers'
    = render :partial => "layouts/gtl", :locals => {:action_url => "show/#{@record.id}"}
- elsif @display == 'container_services' && @showtype != "compare"
    = render :partial => "layouts/gtl", :locals => {:action_url => "show/#{@record.id}"}
- else
    = render :partial => @showtype

