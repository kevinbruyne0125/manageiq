<%= render :partial => "layouts/flash_msg" %>
<table class="style1">
<% if @lastaction == "scan_histories" %>
	<% j = 0 %>
<% else %>
  <% j = 1 %>
<% end %>
<% for i in j...@view.col_order.length %>
		<tr class="<%= cycle('row0', 'row1') %>">
			<td class="key">
				<%= @view.headers[i] %>
			</td>
			<td width="300">
				<%=h(@item.send(@view.col_order[i])) %>
			</td>
		</tr>
	<% end %>
	<tr>
		<th colspan="3" class="footer">
		</th>
	</tr>
</table>
