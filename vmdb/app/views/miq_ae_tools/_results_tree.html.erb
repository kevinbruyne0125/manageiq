<div id="results_tree_div">
  <% if @temp[:results] %>
    <br/>
    <div id="treebox" style="width:auto"></div>
    <%= render(:partial => "layouts/dynatree",
               :locals  => {:tree_id                      => "treebox",
                            :json_tree                    => @temp[:json_tree],
                            :multi_lines                  => true,
                            :exp_tree                     => true,
                            :open_close_all_on_dbl_click  => true
                           }) %>

  <% else %>
    <div class="alert alert-info">
      <span class="pficon pficon-info"></span>
      <strong>Enter Automation Simulation options on the left and press Submit.</strong>
    </div>
  <% end %>
</div>
