    :locals => {:tree_id => "images_treebox",
      :tree_name => "images_tree",
      :id_prefix => "images_",
      :json_tree => @temp[:images_tree],
      :onclick => "cfmeOnClick_SelectTreeNode",
      :select_node=>"#{x_node(:images_tree)}",
      :base_id => "root",
      :no_base_exp => true,
      :exp_tree => false,
      :highlighting => true,
      :tree_state => true,
      :autoload => true})
