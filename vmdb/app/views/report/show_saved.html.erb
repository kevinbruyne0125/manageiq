<div id='main_div'>
<% if @report == nil %>
  <%= render :partial => 'layouts/info_msg', :locals => {:message => "Choose a Report from the menus on the left."}%>
<% else  %>
  <fieldset>
    <p class="legend"><%= @report.name %></p>
    <%= render :partial => "layouts/flash_msg" %>
    <%= render :partial => "shared/report_chart_and_html", :locals => { :html => @html, :chart => @zgraph, :report => @report } %>
  </fieldset>
<% end  %>
</div>
