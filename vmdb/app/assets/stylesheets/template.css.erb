/* begin modified Patternfly code */

.btn {
    display: inline-block;
    margin-bottom: 0px;
    font-weight: 600;
    text-align: center;
    vertical-align: middle;
    cursor: pointer;
    background-image: none;
    border: 1px solid;
    white-space: nowrap;
    padding: 2px 6px;
    font-size: 12px;
    line-height: 1.66667;
    border-radius: 1px;
    -moz-user-select: none;
    box-shadow: 0px 2px 3px rgba(0, 0, 0, 0.1);
}

.btn-default {
    background-color: #EEE;
    background-image: linear-gradient(to bottom, #FAFAFA 0px, #EDEDED 100%);
    background-repeat: repeat-x;
    border-color: #B7B7B7;
    color: #4D5258;
}

.btn-primary {
    background-color: #189AD1;
    background-image: linear-gradient(to bottom, #1CACE8 0px, #1998CC 100%);
    background-repeat: repeat-x;
    border-color: #267DA1;
    color: #FFF;
}

.btn-default:hover, .btn-default:focus, .btn-default:active, .btn-default.active, .open .dropdown-toggle.btn-default {
    background-color: #EEE;
    background-image: none;
    border-color: #B7B7B7;
    color: #4D5258;
}

.btn-primary:hover, .btn-primary:focus, .btn-primary:active, .btn-primary.active, .open .dropdown-toggle.btn-primary {
    background-color: #189AD1;
    background-image: none;
    border-color: #267DA1;
    color: #FFF;
}

.btn-disabled {
  opacity:0.4; 
  -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=40)"; 
  filter:alpha(opacity=40);
  filter: progid:DXImageTransform.Microsoft.Alpha(Opacity=40);
}

p.legend {
  margin-top: 9.5px;
  margin-bottom: 12px;
  font: 15px OpenSansRegular,Arial,Helvetica,sans-serif;
  font-weight: 500;
}

dl.dl-horizontal {
    float: none;
    margin-top: 0px;
    margin-bottom: 20px;
    line-height: 1.66667;
}
dl.dl-horizontal dd {
    margin-left: 180px;
    line-height: 2.2;
}
    
dl.dl-horizontal dt {
    float: left;
    width: 160px;
    clear: left;
    text-align: right;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    font-family:'OpenSansSemibold';
    line-height: 2.2;
}

.breadcrumb {
  list-style: none outside none;
  margin: 0 0 25px 0;
  padding: 8px 0;
  border-radius: 1px
  background-color: transparent;
  font-size: 11px;}
.breadcrumb > li + li:before {
  padding: 0px 9px 0px 7px;
  color: #999;
  content: "";
  font-family: FontAwesome;
  font-size: 11px;}
.breadcrumb > li > a {
    color: #0099D3;}
.breadcrumb > li > a:hover,.breadcrumb > li > a:focus {
    color: #00618A;
    text-decoration: underline;}

hr{
